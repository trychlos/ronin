#meteor build ../ronin.build --architecture os.linux.x86_64
meteor build /tmp --architecture os.linux.x86_64
    scp /tmp/ronin.tar.gz www7:/home/ronin
    ssh www7 (systemctl stop ronin; cd /home/ronin; rm -f bundle; tar -xzvf ronin.tar.gz; chown -R ronin:ronin bundle; tmpd="$(date +'%y%m%d')"; mv bundle bundle-$tmpd; ln -s bundle-$tmpd bundle; systemctl start ronin)

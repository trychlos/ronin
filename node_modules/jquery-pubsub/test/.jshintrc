{
  // Globals for Jasmine tests
  "globals"   : {
    "describe"   : true,
    "expect"     : true,
    "it"         : true,
    "before"     : true,
    "beforeEach" : true,
    "after"      : true,
    "afterEach"  : true,
    "xit"        : true
  },


  // Enforcing options (`true` to enforce)
  "bitwise"     : false,
  "camelcase"   : false,
  "curly"       : true,
  "eqeqeq"      : true,
  "es3"         : true,
  "forin"       : true,
  "immed"       : true,
  "indent"      : 4,
  "latedef"     : true,
  "newcap"      : true,
  "noarg"       : true,
  "noempty"     : false,
  "nonbsp"      : true,
  "nonew"       : false,
  "nomen"       : false,
  "plusplus"    : false,
  "quotmark"    : true,
  "strict"      : true,
  "undef"       : true,
  "unused"      : false,

  // Relaxing options (`true` to relax)
  "asi"         : false,
  "boss"        : true,
  "debug"       : false,
  "eqnull"      : false,
  "esnext"      : false,
  "evil"        : false,
  "expr"        : false,
  "funcscope"   : false,
  "globalstrict": false,
  "iterator"    : false,
  "lastsemic"   : false,
  "laxbreak"    : false,
  "laxcomma"    : true,
  "loopfunc"    : false,
  "maxerr"      : 100,
  "moz"         : true,
  "multistr"    : false,
  "notypeof"    : false,
  "proto"       : false,
  "scripturl"   : false,
  "shadow"      : false,
  "sub"         : false,
  "supernew"    : false,
  "validthis"   : false,
  "noyield"     : false,

  // Environments
  "browser"     : true,
  "couch"       : false,
  "devel"       : true,
  "dojo"        : false,
  "jasmine"     : true,
  "jquery"      : true,
  "mootools"    : false,
  "node"        : true,
  "nonstandard" : false,
  "phantom"     : false,
  "prototypejs" : true,
  "rhino"       : false,
  "worker"      : false,
  "wsh"         : false,
  "yui"         : false

}